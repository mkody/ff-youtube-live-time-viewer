{
  "name": "ff-youtube-live-time-viewer",
  "description": "Firefox Extension for Viewing Time on YouTube Live.",
  "version": "0.0.7",
  "dependencies": {
    "@fiahfy/semaphore": "^0.0.2",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@crxjs/vite-plugin": "^2.0.0-beta.26",
    "@fiahfy/biome-config": "^0.0.1",
    "@tsconfig/vite-react": "^3.4.0",
    "@types/chrome": "^0.0.307",
    "husky": "^9.1.7",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "yarn@4.5.0",
  "private": true,
  "productName": "Time Viewer for YouTube Live",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
